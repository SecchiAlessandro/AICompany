{
  "project": {
    "name": "Agent Workflow Orchestrator",
    "description": "Dynamic agent creation and orchestration based on workflow maps"
  },
  "agents": {
    "directory": ".claude/agents",
    "available": [
      "orchestrator"
    ]
  },
  "workflows": {
    "directory": "workflows",
    "format": "yaml"
  },
  "results": {
    "file": "results/shared.md"
  },
  "statusLine": {
    "type": "command",
    "command": ".claude/statusline.sh"
  },
  "hooks": {
    "Stop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "python3 /Users/aless/Desktop/AI_Projects/AICompany/.claude/scripts/check_okrs.py",
            "timeout": 30
          }
        ]
      }
    ],
    "SubagentStop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "python3 /Users/aless/Desktop/AI_Projects/AICompany/.claude/scripts/check_okrs.py",
            "timeout": 30
          }
        ]
      }
    ]
  }
}

